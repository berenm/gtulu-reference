  name: glBlendFuncSeparatei
  params:
  - mapping: buf
    name: buf
    type: GLuint
  - mapping: srcRGB
    name: srcRGB
    type: GLenum
  - mapping: dstRGB
    name: dstRGB
    type: GLenum
  - mapping: srcAlpha
    name: srcAlpha
    type: GLenum
  - mapping: dstAlpha
    name: dstAlpha
    type: GLenum
  return: void
gtu:
  name: blend_func
  params:
  - name: buf
    type: uint32_t const
  - name: srcRGB
    type: gtu::constant const
  - name: dstRGB
    type: gtu::constant const
  - name: srcAlpha
    type: gtu::constant const
  - name: dstAlpha
