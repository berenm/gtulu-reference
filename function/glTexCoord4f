gl:
  name: glTexCoord4f
  params:
  - mapping: coordinates[0]
    name: s
    type: GLfloat
  - mapping: coordinates[1]
    name: t
    type: GLfloat
  - mapping: coordinates[2]
    name: r
    type: GLfloat
  - mapping: coordinates[3]
    name: q
    type: GLfloat
  return: void
gtu:
  name: tex_coord
  params:
  - name: coordinates
    type: gtu::vec4< float32 > const&
  return: void
  skip: true
