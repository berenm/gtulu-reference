gl:
  name: glDrawArraysInstanced
  params:
  - mapping: mode
    name: mode
    type: GLenum
  - mapping: first
    name: first
    type: GLint
  - mapping: count
    name: count
    type: GLsizei
  - mapping: primitive_count
    name: instancecount
    type: GLsizei
  return: void
gtu:
  name: draw_arrays
  params:
  - name: mode
    type: gtu::constant const
  - name: count
    type: gtu::size const
  - name: primitive_count
    type: gtu::size const
  - name: first
    type: int32_t const
  return: void
