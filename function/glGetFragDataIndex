gl:
  name: glGetFragDataIndex
  params:
  - mapping: program
    name: program
    type: GLuint
  - mapping: '&name[0]'
    name: name
    type: char const*
  return: GLint
gtu:
  name: get_frag_data_index
  params:
  - name: program
    type: gtu::program const
  - name: name
    type: std::string const&
  return: gtu::frag_data_index
