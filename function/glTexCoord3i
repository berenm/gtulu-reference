gl:
  name: glTexCoord3i
  params:
  - mapping: coordinates[0]
    name: s
    type: GLint
  - mapping: coordinates[1]
    name: t
    type: GLint
  - mapping: coordinates[2]
    name: r
    type: GLint
  return: void
gtu:
  name: tex_coord
  params:
  - name: coordinates
    type: gtu::vec3< int32_t > const&
  return: void
  skip: true
