gl:
  name: glVertexAttrib1f
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: values[0]
    name: x
    type: GLfloat
  return: void
gtu:
  name: vertex_attrib
  params:
  - name: vertex_attrib
    type: gtu::vertex_attrib const
  - name: values
    type: gtu::vec1< float32 > const&
  return: void
  skip: true
