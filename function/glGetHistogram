gl:
  name: glGetHistogram
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: reset
    name: reset
    type: GLboolean
  - mapping: format
    name: format
    type: GLenum
  - mapping: type
    name: type
    type: GLenum
  - mapping: data
    name: values
    type: GLvoid*
  return: void
gtu:
  name: get_histogram
  params:
  - name: target
    type: gtu::constant const
  - name: reset
    type: bool const
  - name: format
    type: gtu::constant const
  - name: type
    type: gtu::constant const
  - name: data
    type: gtu::buffer_ref&
  return: void
