gl:
  name: glVertexAttribI4iv
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: '&values[0]'
    name: v
    type: GLint const*
  return: void
gtu:
  name: vertex_attrib
  params:
  - name: vertex_attrib
    type: gtu::vertex_integer const
  - name: values
    type: gtu::vec4< int32_t > const&
  return: void
