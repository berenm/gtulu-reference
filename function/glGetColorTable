gl:
  name: glGetColorTable
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: format
    name: format
    type: GLenum
  - mapping: type
    name: type
    type: GLenum
  - mapping: data
    name: table
    type: GLvoid*
  return: void
gtu:
  name: get_color_table
  params:
  - name: target
    type: gtu::constant const
  - name: format
    type: gtu::constant const
  - name: type
    type: gtu::constant const
  - name: data
    type: gtu::buffer_ref&
  return: void
