gl:
  name: glRotated
  params:
  - mapping: angle
    name: angle
    type: GLdouble
  - mapping: direction[0]
    name: x
    type: GLdouble
  - mapping: direction[1]
    name: y
    type: GLdouble
  - mapping: direction[2]
    name: z
    type: GLdouble
  return: void
gtu:
  name: rotate
  params:
  - name: angle
    type: float64 const
  - name: direction
    type: gtu::vec3< float64 > const&
  return: void
