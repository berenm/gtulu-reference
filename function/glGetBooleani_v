gl:
  name: glGetBooleani_v
  params:
  - mapping: property
    name: pname
    type: GLenum
  - mapping: index
    name: index
    type: GLuint
  - mapping: data
    name: data
    type: GLboolean*
  return: void
gtu:
  name: get
  params:
  - name: property
    type: gtu::constant const
  - name: index
    type: uint32_t const
  - name: data
    type: uint8_t*
  return: void
