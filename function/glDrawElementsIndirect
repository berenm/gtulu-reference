gl:
  name: glDrawElementsIndirect
  params:
  - mapping: mode
    name: mode
    type: GLenum
  - mapping: type
    name: type
    type: GLenum
  - mapping: elements
    name: indirect
    type: void const*
  return: void
gtu:
  name: draw_elements
  params:
  - name: mode
    type: gtu::constant const
  - name: type
    type: gtu::constant const
  - name: elements
    type: gtu::buffer_ref const&
  return: void
