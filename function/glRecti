gl:
  name: glRecti
  params:
  - mapping: x1
    name: x1
    type: GLint
  - mapping: y1
    name: y1
    type: GLint
  - mapping: x2
    name: x2
    type: GLint
  - mapping: y2
    name: y2
    type: GLint
  return: void
gtu:
  name: rect
  params:
  - name: x1
    type: int32_t const
  - name: y1
    type: int32_t const
  - name: x2
    type: int32_t const
  - name: y2
    type: int32_t const
  return: void
