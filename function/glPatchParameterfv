gl:
  name: glPatchParameterfv
  params:
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&value[0]'
    name: values
    type: GLfloat const*
  return: void
gtu:
  name: patch_parameter
  params:
  - name: property
    type: gtu::constant const
  - name: value
    type: gtu::vec1< float32 > const&
  return: void
