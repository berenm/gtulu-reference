gl:
  name: glSamplerParameterIiv
  params:
  - mapping: sampler
    name: sampler
    type: GLuint
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&values[0]'
    name: params
    type: GLint const*
  return: void
gtu:
  name: sampler_parameter_integer
  params:
  - name: sampler
    type: gtu::sampler const
  - name: property
    type: gtu::constant const
  - name: values
    type: gtu::vec4< int32_t > const&
  return: void
