gl:
  name: glVertexAttrib4Nuiv
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: '&values[0]'
    name: v
    type: GLuint const*
  return: void
gtu:
  name: vertex_attrib_normalized
  params:
  - name: vertex_attrib
    type: gtu::vertex_attrib const
  - name: values
    type: gtu::vec4< uint32_t > const&
  return: void
