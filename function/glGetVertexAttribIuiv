gl:
  name: glGetVertexAttribIuiv
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&values[0]'
    name: params
    type: GLuint*
  return: void
gtu:
  name: get_vertex_attrib_integer
  params:
  - name: vertex_attrib
    type: gtu::vertex_attrib const
  - name: property
    type: gtu::constant const
  - name: values
    type: gtu::vec4< uint32_t >&
  return: void
