gl:
  name: glSecondaryColor3s
  params:
  - mapping: color[0]
    name: red
    type: GLshort
  - mapping: color[1]
    name: green
    type: GLshort
  - mapping: color[2]
    name: blue
    type: GLshort
  return: void
