gl:
  name: glLightModelfv
  params:
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&values[0]'
    name: params
    type: GLfloat const*
  return: void
gtu:
  name: light_model
  params:
  - name: property
    type: gtu::constant const
  - name: values
    type: gtu::vec4< float32 > const&
  return: void
