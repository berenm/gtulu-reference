gl:
  name: glCopyTexImage1D
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: level
    name: level
    type: GLint
  - mapping: internal_format
    name: internalformat
    type: GLenum
  - mapping: position[0]
    name: x
    type: GLint
  - mapping: position[1]
    name: y
    type: GLint
  - mapping: dimension[0]
    name: width
    type: GLsizei
  - mapping: border
    name: border
    type: GLint
  return: void
gtu:
  name: copy_tex_image
  params:
  - name: target
    type: gtu::constant const
  - name: level
    type: int32_t const
  - name: internal_format
    type: gtu::constant const
  - name: position
    type: gtu::vec2< int32_t > const&
  - name: dimension
    type: gtu::vec1< gtu::size > const&
  - name: border
    type: int32_t const
  return: void
