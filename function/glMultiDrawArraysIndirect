gl:
  name: glMultiDrawArraysIndirect
  params:
  - mapping: mode
    name: mode
    type: GLenum
  - mapping: arrays
    name: indirect
    type: void const*
  - mapping: count
    name: drawcount
    type: GLsizei
  - mapping: stride
    name: stride
    type: GLsizei
  return: void
gtu:
  name: draw_arrays
  params:
  - name: mode
    type: gtu::constant const
  - name: arrays
    type: gtu::buffer_ref const&
  - name: count
    type: gtu::size const
  - name: stride
    type: gtu::size const
  return: void
