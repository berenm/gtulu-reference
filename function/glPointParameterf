gl:
  name: glPointParameterf
  params:
  - mapping: property
    name: pname
    type: GLenum
  - mapping: param
    name: param
    type: GLfloat
  return: void
gtu:
  name: point_parameter
  params:
  - name: property
    type: gtu::constant const
  - name: param
    type: float32 const
  return: void
