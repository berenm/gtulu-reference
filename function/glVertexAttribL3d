gl:
  name: glVertexAttribL3d
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: values[0]
    name: v0
    type: GLdouble
  - mapping: values[1]
    name: v1
    type: GLdouble
  - mapping: values[2]
    name: v2
    type: GLdouble
  return: void
gtu:
  name: vertex_attrib_large
  params:
  - name: vertex_attrib
    type: gtu::vertex_attrib const
  - name: values
    type: gtu::vec3< float64 > const&
  return: void
  skip: true
