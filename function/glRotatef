gl:
  name: glRotatef
  params:
  - mapping: angle
    name: angle
    type: GLfloat
  - mapping: direction[0]
    name: x
    type: GLfloat
  - mapping: direction[1]
    name: y
    type: GLfloat
  - mapping: direction[2]
    name: z
    type: GLfloat
  return: void
gtu:
  name: rotate
  params:
  - name: angle
    type: float32 const
  - name: direction
    type: gtu::vec3< float32 > const&
  return: void
