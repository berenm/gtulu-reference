gl:
  name: glUniform3f
  params:
  - mapping: uniform
    name: location
    type: GLint
  - mapping: values[0]
    name: v0
    type: GLfloat
  - mapping: values[1]
    name: v1
    type: GLfloat
  - mapping: values[2]
    name: v2
    type: GLfloat
  return: void
gtu:
  name: uniform
  params:
  - name: uniform
    type: gtu::uniform const
  - name: values
    type: gtu::vec3< float32 > const&
  return: void
