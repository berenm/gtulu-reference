gl:
  name: glGetIntegerv
  params:
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&params[0]'
    name: params
    type: GLint*
  return: void
gtu:
  name: get
  params:
  - name: property
    type: gtu::constant const
  - name: params
    type: std::vector< int32_t >&
  return: void
