gl:
  name: glGetStringi
  params:
  - mapping: property
    name: name
    type: GLenum
  - mapping: index
    name: index
    type: GLuint
  return: const GLubyte*
gtu:
  name: get_string
  params:
  - name: property
    type: gtu::constant const
  - name: index
    type: uint32_t const
  postcall: |
    return __r == nullptr ? std::string() : std::string(static_cast< char const* >(static_cast< void const* >(__r)));
  return: std::string
