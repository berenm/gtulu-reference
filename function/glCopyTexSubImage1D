gl:
  name: glCopyTexSubImage1D
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: level
    name: level
    type: GLint
  - mapping: offset[0]
    name: xoffset
    type: GLint
  - mapping: position[0]
    name: x
    type: GLint
  - mapping: position[1]
    name: y
    type: GLint
  - mapping: dimension[0]
    name: width
    type: GLsizei
  return: void
gtu:
  name: copy_tex_sub_image
  params:
  - name: target
    type: gtu::constant const
  - name: level
    type: int32_t const
  - name: offset
    type: gtu::vec1< int32_t > const&
  - name: position
    type: gtu::vec2< int32_t > const&
  - name: dimension
    type: gtu::vec1< gtu::size > const&
  return: void
