gl:
  name: glCopyColorSubTable
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: start
    name: start
    type: GLsizei
  - mapping: position[0]
    name: x
    type: GLint
  - mapping: position[1]
    name: y
    type: GLint
  - mapping: dimension[0]
    name: width
    type: GLsizei
  return: void
gtu:
  name: copy_color_sub_table
  params:
  - name: target
    type: gtu::constant const
  - name: start
    type: gtu::size const
  - name: position
    type: gtu::vec2< int32_t > const&
  - name: dimension
    type: gtu::vec1< gtu::size > const&
  return: void
