gl:
  name: glSecondaryColor3us
  params:
  - mapping: color[0]
    name: red
    type: GLushort
  - mapping: color[1]
    name: green
    type: GLushort
  - mapping: color[2]
    name: blue
    type: GLushort
  return: void
gtu:
  name: secondary_color
  params:
  - name: color
    type: gtu::vec3< uint16_t > const&
  return: void
  skip: true
