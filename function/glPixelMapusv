gl:
  name: glPixelMapusv
  params:
  - mapping: map
    name: map
    type: GLenum
  - mapping: values.size()
    name: mapsize
    type: GLsizei
  - mapping: '&values[0]'
    name: values
    type: GLushort const*
  return: void
gtu:
  name: pixel_map
  params:
  - name: map
    type: gtu::constant const
  - name: values
    type: std::vector< uint16_t > const&
  return: void
