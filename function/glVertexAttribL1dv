gl:
  name: glVertexAttribL1dv
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: '&values[0]'
    name: v
    type: GLdouble const*
  return: void
gtu:
  name: vertex_attrib
  params:
  - name: vertex_attrib
    type: gtu::vertex_double const
  - name: values
    type: gtu::vec1< float64 > const&
  return: void
