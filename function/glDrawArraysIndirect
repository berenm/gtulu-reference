gl:
  name: glDrawArraysIndirect
  params:
  - mapping: mode
    name: mode
    type: GLenum
  - mapping: arrays
    name: indirect
    type: void const*
  return: void
gtu:
  name: draw_arrays
  params:
  - name: mode
    type: gtu::constant const
  - name: arrays
    type: gtu::buffer_ref const&
  return: void
