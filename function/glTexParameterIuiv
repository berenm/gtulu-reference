gl:
  name: glTexParameterIuiv
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&values[0]'
    name: params
    type: GLuint const*
  return: void
gtu:
  name: tex_parameter_integer
  params:
  - name: target
    type: gtu::constant const
  - name: property
    type: gtu::constant const
  - name: values
    type: gtu::vec4< uint32_t > const&
  return: void
