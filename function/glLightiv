gl:
  name: glLightiv
  params:
  - mapping: light
    name: light
    type: GLenum
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&values[0]'
    name: params
    type: GLint const*
  return: void
gtu:
  name: light
  params:
  - name: light
    type: gtu::constant const
  - name: property
    type: gtu::constant const
  - name: values
    type: gtu::vec4< int32_t > const&
  return: void
