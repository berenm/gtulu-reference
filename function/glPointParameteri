gl:
  name: glPointParameteri
  params:
  - mapping: property
    name: pname
    type: GLenum
  - mapping: param
    name: param
    type: GLint
  return: void
gtu:
  name: point_parameter
  params:
  - name: property
    type: gtu::constant const
  - name: param
    type: int32_t const
  return: void
