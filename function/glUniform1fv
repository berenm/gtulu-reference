gl:
  name: glUniform1fv
  params:
  - mapping: uniform
    name: location
    type: GLint
  - mapping: values.size()
    name: count
    type: GLsizei
  - mapping: '&values[0]'
    name: value
    type: GLfloat const*
  return: void
gtu:
  name: uniform
  params:
  - name: uniform
    type: gtu::uniform const
  - name: values
    type: std::vector< float32 > const&
  return: void
