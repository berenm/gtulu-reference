gl:
  name: glScissor
  params:
  - mapping: position[0]
    name: x
    type: GLint
  - mapping: position[1]
    name: y
    type: GLint
  - mapping: dimension[0]
    name: width
    type: GLsizei
  - mapping: dimension[1]
    name: height
    type: GLsizei
  return: void
gtu:
  name: scissor
  params:
  - name: position
    type: gtu::vec2< int32_t > const&
  - name: dimension
    type: gtu::vec2< gtu::size > const&
  return: void
