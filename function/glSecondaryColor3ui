gl:
  name: glSecondaryColor3ui
  params:
  - mapping: color[0]
    name: red
    type: GLuint
  - mapping: color[1]
    name: green
    type: GLuint
  - mapping: color[2]
    name: blue
    type: GLuint
  return: void
gtu:
  name: secondary_color
  params:
  - name: color
    type: gtu::vec3< uint32_t > const&
  return: void
  skip: true
