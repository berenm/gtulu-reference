gl:
  name: glGetAttribLocation
  params:
  - mapping: program
    name: program
    type: GLuint
  - mapping: '&name[0]'
    name: name
    type: GLchar const*
  return: GLint
gtu:
  name: get_attrib_location
  params:
  - name: program
    type: gtu::program const
  - name: name
    type: std::string const&
  return: gtu::vertex_attrib
