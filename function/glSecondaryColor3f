gl:
  name: glSecondaryColor3f
  params:
  - mapping: color[0]
    name: red
    type: GLfloat
  - mapping: color[1]
    name: green
    type: GLfloat
  - mapping: color[2]
    name: blue
    type: GLfloat
  return: void
gtu:
  name: secondary_color
  params:
  - name: color
    type: gtu::vec3< float32 > const&
  return: void
  skip: true
