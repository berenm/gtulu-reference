gl:
  name: glTexGeniv
  params:
  - mapping: coord
    name: coord
    type: GLenum
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&values[0]'
    name: params
    type: GLint const*
  return: void
gtu:
  name: tex_gen
  params:
  - name: coord
    type: gtu::constant const
  - name: property
    type: gtu::constant const
  - name: values
    type: gtu::vec4< int32_t > const&
  return: void
