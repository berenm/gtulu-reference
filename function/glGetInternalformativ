gl:
  name: glGetInternalformativ
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: internal_format
    name: internalformat
    type: GLenum
  - mapping: property
    name: pname
    type: GLenum
  - mapping: values.size()
    name: bufSize
    type: GLsizei
  - mapping: '&values[0]'
    name: params
    type: GLint*
  return: void
gtu:
  name: get_internal_format
  params:
  - name: target
    type: gtu::constant const
  - name: internal_format
    type: gtu::constant const
  - name: property
    type: gtu::constant const
  - name: values
    type: std::vector< int32_t >&
  return: void
