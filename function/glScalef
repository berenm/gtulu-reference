gl:
  name: glScalef
  params:
  - mapping: proportion[0]
    name: x
    type: GLfloat
  - mapping: proportion[1]
    name: y
    type: GLfloat
  - mapping: proportion[2]
    name: z
    type: GLfloat
  return: void
gtu:
  name: scale
  params:
  - name: proportion
    type: gtu::vec3< float32 > const&
  return: void
