gl:
  name: glGetSubroutineIndex
  params:
  - mapping: program
    name: program
    type: GLuint
  - mapping: shadertype
    name: shadertype
    type: GLenum
  - mapping: '&name[0]'
    name: name
    type: GLchar const*
  return: GLuint
gtu:
  name: get_subroutine_index
  params:
  - name: program
    type: gtu::program const
  - name: shadertype
    type: gtu::constant const
  - name: name
    type: std::string const&
  return: gtu::subroutine
