gl:
  name: glTransformFeedbackVaryings
  params:
  - mapping: program
    name: program
    type: GLuint
  - mapping: varyings.size()
    name: count
    type: GLsizei
  - mapping: '&strings[0]'
    name: varyings
    type: char const* const*
  - mapping: bufferMode
    name: bufferMode
    type: GLenum
  return: void
gtu:
  name: transform_feedback_varyings
  params:
  - name: program
    type: gtu::program const
  - name: varyings
    type: std::vector< std::string > const&
  - name: bufferMode
    type: gtu::constant const
  precall: |
    std::vector< char const* >  strings;
    std::transform(varyings.begin(), varyings.end(), std::back_inserter(strings), [](std::string const& e) { return e.data(); });
  return: void
