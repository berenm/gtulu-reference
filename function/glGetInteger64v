gl:
  name: glGetInteger64v
  params:
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&params[0]'
    name: params
    type: GLint64*
  return: void
gtu:
  name: get
  params:
  - name: property
    type: gtu::constant const
  - name: params
    type: std::vector< int64_t >&
  return: void
