gl:
  name: glVertexAttrib4s
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: values[0]
    name: x
    type: GLshort
  - mapping: values[1]
    name: y
    type: GLshort
  - mapping: values[2]
    name: z
    type: GLshort
  - mapping: values[3]
    name: w
    type: GLshort
  return: void
gtu:
  name: vertex_attrib
  params:
  - name: vertex_attrib
    type: gtu::vertex_attrib const
  - name: values
    type: gtu::vec4< int16_t > const&
  return: void
  skip: true
