gl:
  name: glGetDoublev
  params:
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&params[0]'
    name: params
    type: GLdouble*
  return: void
gtu:
  name: get
  params:
  - name: property
    type: gtu::constant const
  - name: params
    type: std::vector< float64 >&
  return: void
