gl:
  name: glSamplerParameterfv
  params:
  - mapping: sampler
    name: sampler
    type: GLuint
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&values[0]'
    name: params
    type: GLfloat const*
  return: void
gtu:
  name: sampler_parameter
  params:
  - name: sampler
    type: gtu::sampler const
  - name: property
    type: gtu::constant const
  - name: values
    type: gtu::vec4< float32 > const&
  return: void
