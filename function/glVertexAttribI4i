gl:
  name: glVertexAttribI4i
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: values[0]
    name: x
    type: GLint
  - mapping: values[1]
    name: y
    type: GLint
  - mapping: values[2]
    name: z
    type: GLint
  - mapping: values[3]
    name: w
    type: GLint
  return: void
gtu:
  name: vertex_attrib_integer
  params:
  - name: vertex_attrib
    type: gtu::vertex_attrib const
  - name: values
    type: gtu::vec4< int32_t > const&
  return: void
  skip: true
