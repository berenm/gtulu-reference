gl:
  name: glConvolutionParameteriv
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&values[0]'
    name: params
    type: GLint const*
  return: void
gtu:
  name: convolution_parameter
  params:
  - name: target
    type: gtu::constant const
  - name: property
    type: gtu::constant const
  - name: values
    type: gtu::vec4< int32_t > const&
  return: void
