gl:
  name: glVertexAttribI2i
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: values[0]
    name: x
    type: GLint
  - mapping: values[1]
    name: y
    type: GLint
  return: void
gtu:
  name: vertex_attrib
  params:
  - name: vertex_attrib
    type: gtu::vertex_integer const
  - name: values
    type: gtu::vec2< int32_t > const&
  return: void
  skip: true
