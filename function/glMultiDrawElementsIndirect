gl:
  name: glMultiDrawElementsIndirect
  params:
  - mapping: mode
    name: mode
    type: GLenum
  - mapping: type
    name: type
    type: GLenum
  - mapping: elements
    name: indirect
    type: void const*
  - mapping: count
    name: drawcount
    type: GLsizei
  - mapping: stride
    name: stride
    type: GLsizei
  return: void
gtu:
  name: draw_elements
  params:
  - name: mode
    type: gtu::constant const
  - name: type
    type: gtu::constant const
  - name: elements
    type: gtu::buffer_ref const&
  - name: count
    type: gtu::size const
  - name: stride
    type: gtu::size const
  return: void
