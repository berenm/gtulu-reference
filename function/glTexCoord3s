gl:
  name: glTexCoord3s
  params:
  - mapping: coordinates[0]
    name: s
    type: GLshort
  - mapping: coordinates[1]
    name: t
    type: GLshort
  - mapping: coordinates[2]
    name: r
    type: GLshort
  return: void
gtu:
  name: tex_coord
  params:
  - name: coordinates
    type: gtu::vec3< int16_t > const&
  return: void
  skip: true
