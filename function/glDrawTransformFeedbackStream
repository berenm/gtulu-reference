gl:
  name: glDrawTransformFeedbackStream
  params:
  - mapping: mode
    name: mode
    type: GLenum
  - mapping: transform_feedback
    name: id
    type: GLuint
  - mapping: stream
    name: stream
    type: GLuint
  return: void
gtu:
  name: draw_transform_feedback_stream
  params:
  - name: mode
    type: gtu::constant const
  - name: transform_feedback
    type: gtu::transform_feedback const
  - name: stream
    type: uint32_t const
  return: void
