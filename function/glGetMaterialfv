gl:
  name: glGetMaterialfv
  params:
  - mapping: face
    name: face
    type: GLenum
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&values[0]'
    name: params
    type: GLfloat*
  return: void
gtu:
  name: get_material
  params:
  - name: face
    type: gtu::constant const
  - name: property
    type: gtu::constant const
  - name: values
    type: gtu::vec4< float32 >&
  return: void
