gl:
  name: glClipPlane
  params:
  - mapping: plane
    name: plane
    type: GLenum
  - mapping: '&equation[0]'
    name: equation
    type: GLdouble const*
  return: void
gtu:
  name: clip_plane
  params:
  - name: plane
    type: gtu::constant const
  - name: equation
    type: gtu::vec4< float64 > const&
  return: void
