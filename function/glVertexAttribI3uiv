gl:
  name: glVertexAttribI3uiv
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: '&values[0]'
    name: v
    type: GLuint const*
  return: void
gtu:
  name: vertex_attrib_integer
  params:
  - name: vertex_attrib
    type: gtu::vertex_attrib const
  - name: values
    type: gtu::vec3< uint32_t > const&
  return: void
