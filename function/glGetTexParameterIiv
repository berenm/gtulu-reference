gl:
  name: glGetTexParameterIiv
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&params[0]'
    name: params
    type: GLint*
  return: void
gtu:
  name: get_tex_parameter_integer
  params:
  - name: target
    type: gtu::constant const
  - name: property
    type: gtu::constant const
  - name: params
    type: gtu::vec4< int32_t >&
  return: void
