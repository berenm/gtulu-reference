gl:
  name: glGetTexParameterIiv
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: property
    name: pname
    type: GLenum
  - mapping: params
    name: params
    type: GLint*
  return: void
gtu:
  name: get_tex_parameter_integer
  params:
  - name: target
    type: gtu::constant const
  - name: property
    type: gtu::constant const
  - name: params
    type: int32_t*
  return: void
