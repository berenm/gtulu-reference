gl:
  name: glDrawTransformFeedbackStreamInstanced
  params:
  - mapping: mode
    name: mode
    type: GLenum
  - mapping: transform_feedback
    name: id
    type: GLuint
  - mapping: stream
    name: stream
    type: GLuint
  - mapping: instance_count
    name: primcount
    type: GLsizei
  return: void
gtu:
  name: draw_transform_feedback
  params:
  - name: mode
    type: gtu::constant const
  - name: transform_feedback
    type: gtu::transform_feedback const
  - name: stream
    type: uint32_t const
  - name: instance_count
    type: gtu::size const
  return: void
