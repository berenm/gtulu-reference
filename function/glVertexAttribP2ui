gl:
  name: glVertexAttribP2ui
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: type
    name: type
    type: GLenum
  - mapping: normalized
    name: normalized
    type: GLboolean
  - mapping: value
    name: value
    type: GLuint
  return: void
gtu:
  name: vertex_attrib_packed_2
  params:
  - name: vertex_attrib
    type: gtu::vertex_float const
  - name: type
    type: gtu::constant const
  - name: normalized
    type: bool const
  - name: value
    type: uint32_t const
  return: void
