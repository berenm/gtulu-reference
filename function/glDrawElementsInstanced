gl:
  name: glDrawElementsInstanced
  params:
  - mapping: mode
    name: mode
    type: GLenum
  - mapping: count
    name: count
    type: GLsizei
  - mapping: type
    name: type
    type: GLenum
  - mapping: indices
    name: indices
    type: void const*
  - mapping: primitive_count
    name: instancecount
    type: GLsizei
  return: void
gtu:
  name: draw_elements
  params:
  - name: mode
    type: gtu::constant const
  - name: type
    type: gtu::constant const
  - name: count
    type: gtu::size const
  - name: primitive_count
    type: gtu::size const
  - name: indices
    type: gtu::buffer_ref const&
  return: void
