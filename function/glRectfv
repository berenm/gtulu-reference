gl:
  name: glRectfv
  params:
  - mapping: '&v1[0]'
    name: v1
    type: GLfloat const*
  - mapping: '&v2[0]'
    name: v2
    type: GLfloat const*
  return: void
gtu:
  name: rect
  params:
  - name: v1
    type: gtu::vec2< float32 > const&
  - name: v2
    type: gtu::vec2< float32 > const&
  return: void
