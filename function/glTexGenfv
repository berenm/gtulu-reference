gl:
  name: glTexGenfv
  params:
  - mapping: coord
    name: coord
    type: GLenum
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&values[0]'
    name: params
    type: GLfloat const*
  return: void
gtu:
  name: tex_gen
  params:
  - name: coord
    type: gtu::constant const
  - name: property
    type: gtu::constant const
  - name: values
    type: gtu::vec4< float32 > const&
  return: void
