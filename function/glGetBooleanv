gl:
  name: glGetBooleanv
  params:
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&params[0]'
    name: params
    type: GLboolean*
  return: void
gtu:
  name: get
  params:
  - name: property
    type: gtu::constant const
  - name: params
    type: std::vector< uint8_t >&
  return: void
