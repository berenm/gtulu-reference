gl:
  name: glViewportArrayv
  params:
  - mapping: first
    name: first
    type: GLuint
  - mapping: values.size()
    name: count
    type: GLsizei
  - mapping: '&values[0][0]'
    name: v
    type: GLfloat const*
  return: void
gtu:
  name: viewport_array
  params:
  - name: first
    type: uint32_t const
  - name: values
    type: std::vector< gtu::vec4< float32 > > const&
  return: void
