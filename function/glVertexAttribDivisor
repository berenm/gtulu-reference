gl:
  name: glVertexAttribDivisor
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: divisor
    name: divisor
    type: GLuint
  return: void
gtu:
  name: vertex_attrib_divisor
  params:
  - name: vertex_attrib
    type: gtu::vertex_attrib const
  - name: divisor
    type: uint32_t const
  return: void
