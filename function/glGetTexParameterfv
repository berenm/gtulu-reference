gl:
  name: glGetTexParameterfv
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&params[0]'
    name: params
    type: GLfloat*
  return: void
gtu:
  name: get_tex_parameter
  params:
  - name: target
    type: gtu::constant const
  - name: property
    type: gtu::constant const
  - name: params
    type: gtu::vec4< float32 >&
  return: void
