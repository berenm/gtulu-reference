gl:
  name: glIndexPointer
  params:
  - mapping: type
    name: type
    type: GLenum
  - mapping: stride
    name: stride
    type: GLsizei
  - mapping: data
    name: pointer
    type: GLvoid const*
  return: void
gtu:
  name: index_pointer
  params:
  - name: type
    type: gtu::constant const
  - name: stride
    type: gtu::size const
  - name: data
    type: gtu::buffer_ref const&
  return: void
