gl:
  name: glMultiTexCoord4f
  params:
  - mapping: texture_unit
    name: target
    type: GLenum
  - mapping: coordinates[0]
    name: s
    type: GLfloat
  - mapping: coordinates[1]
    name: t
    type: GLfloat
  - mapping: coordinates[2]
    name: r
    type: GLfloat
  - mapping: coordinates[3]
    name: q
    type: GLfloat
  return: void
gtu:
  name: multi_tex_coord
  params:
  - name: texture_unit
    type: gtu::texture_unit const
  - name: coordinates
    type: gtu::vec4< float32 > const&
  return: void
  skip: true
