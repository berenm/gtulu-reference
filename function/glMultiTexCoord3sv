gl:
  name: glMultiTexCoord3sv
  params:
  - mapping: texture_unit
    name: target
    type: GLenum
  - mapping: '&coordinates[0]'
    name: v
    type: GLshort const*
  return: void
gtu:
  name: multi_tex_coord
  params:
  - name: texture_unit
    type: gtu::texture_unit const
  - name: coordinates
    type: gtu::vec3< int16_t > const&
  return: void
