gl:
  name: glProgramUniform3i
  params:
  - mapping: program
    name: program
    type: GLuint
  - mapping: uniform
    name: location
    type: GLint
  - mapping: values[0]
    name: v0
    type: GLint
  - mapping: values[1]
    name: v1
    type: GLint
  - mapping: values[2]
    name: v2
    type: GLint
  return: void
gtu:
  name: program_uniform
  params:
  - name: program
    type: gtu::program const
  - name: uniform
    type: gtu::uniform const
  - name: values
    type: gtu::vec3< int32_t > const&
  return: void
