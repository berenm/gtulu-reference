gl:
  name: glTranslated
  params:
  - mapping: direction[0]
    name: x
    type: GLdouble
  - mapping: direction[1]
    name: y
    type: GLdouble
  - mapping: direction[2]
    name: z
    type: GLdouble
  return: void
gtu:
  name: translate
  params:
  - name: direction
    type: gtu::vec3< float64 > const&
  return: void
