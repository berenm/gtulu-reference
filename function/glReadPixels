gl:
  name: glReadPixels
  params:
  - mapping: position[0]
    name: x
    type: GLint
  - mapping: position[1]
    name: y
    type: GLint
  - mapping: dimension[0]
    name: width
    type: GLsizei
  - mapping: dimension[1]
    name: height
    type: GLsizei
  - mapping: format
    name: format
    type: GLenum
  - mapping: type
    name: type
    type: GLenum
  - mapping: data
    name: data
    type: GLvoid*
  return: void
gtu:
  name: read_pixels
  params:
  - name: position
    type: gtu::vec2< int32_t > const&
  - name: dimension
    type: gtu::vec2< gtu::size > const&
  - name: format
    type: gtu::constant const
  - name: type
    type: gtu::constant const
  - name: data
    type: gtu::bytearray&
  return: void
