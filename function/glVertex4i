gl:
  name: glVertex4i
  params:
  - mapping: position[0]
    name: x
    type: GLint
  - mapping: position[1]
    name: y
    type: GLint
  - mapping: position[2]
    name: z
    type: GLint
  - mapping: position[3]
    name: w
    type: GLint
  return: void
gtu:
  name: vertex
  params:
  - name: position
    type: gtu::vec3< int32_t > const&
  return: void
  skip: true
