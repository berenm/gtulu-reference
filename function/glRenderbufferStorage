gl:
  name: glRenderbufferStorage
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: internalformat
    name: internalformat
    type: GLenum
  - mapping: dimension[0]
    name: width
    type: GLsizei
  - mapping: dimension[1]
    name: height
    type: GLsizei
  return: void
gtu:
  name: renderbuffer_storage
  params:
  - name: target
    type: gtu::constant const
  - name: internalformat
    type: gtu::constant const
  - name: dimension
    type: gtu::vec2< gtu::size > const&
  return: void
