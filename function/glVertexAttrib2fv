gl:
  name: glVertexAttrib2fv
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: '&values[0]'
    name: v
    type: GLfloat const*
  return: void
gtu:
  name: vertex_attrib
  params:
  - name: vertex_attrib
    type: gtu::vertex_float const
  - name: values
    type: gtu::vec2< float32 > const&
  return: void
