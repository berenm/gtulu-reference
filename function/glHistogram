gl:
  name: glHistogram
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: dimension[0]
    name: width
    type: GLsizei
  - mapping: internalformat
    name: internalformat
    type: GLenum
  - mapping: sink
    name: sink
    type: GLboolean
  return: void
gtu:
  name: histogram
  params:
  - name: target
    type: gtu::constant const
  - name: dimension
    type: gtu::vec1< gtu::size > const&
  - name: internalformat
    type: gtu::constant const
  - name: sink
    type: bool const
  return: void
