gl:
  name: glVertexBindingDivisor
  params:
  - mapping: vertex_binding
    name: bindingindex
    type: GLuint
  - mapping: divisor
    name: divisor
    type: GLuint
  return: void
gtu:
  name: vertex_binding_divisor
  params:
  - name: vertex_binding
    type: gtu::vertex_binding const
  - name: divisor
    type: uint32_t const
  return: void
