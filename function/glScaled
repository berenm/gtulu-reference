gl:
  name: glScaled
  params:
  - mapping: proportion[0]
    name: x
    type: GLdouble
  - mapping: proportion[1]
    name: y
    type: GLdouble
  - mapping: proportion[2]
    name: z
    type: GLdouble
  return: void
gtu:
  name: scale
  params:
  - name: proportion
    type: gtu::vec3< float64 > const&
  return: void
