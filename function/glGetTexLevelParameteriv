gl:
  name: glGetTexLevelParameteriv
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: level
    name: level
    type: GLint
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&values[0]'
    name: params
    type: GLint*
  return: void
gtu:
  name: get_tex_level_parameter
  params:
  - name: target
    type: gtu::constant const
  - name: level
    type: int32_t const
  - name: property
    type: gtu::constant const
  - name: values
    type: gtu::vec1< int32_t >&
  return: void
