gl:
  name: glRectdv
  params:
  - mapping: '&v1[0]'
    name: v1
    type: GLdouble const*
  - mapping: '&v2[0]'
    name: v2
    type: GLdouble const*
  return: void
gtu:
  name: rect
  params:
  - name: v1
    type: gtu::vec2< float64 > const&
  - name: v2
    type: gtu::vec2< float64 > const&
  return: void
