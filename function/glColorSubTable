gl:
  name: glColorSubTable
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: start
    name: start
    type: GLsizei
  - mapping: count
    name: count
    type: GLsizei
  - mapping: format
    name: format
    type: GLenum
  - mapping: type
    name: type
    type: GLenum
  - mapping: data
    name: data
    type: GLvoid const*
  return: void
gtu:
  name: color_sub_table
  params:
  - name: target
    type: gtu::constant const
  - name: start
    type: gtu::size const
  - name: count
    type: gtu::size const
  - name: format
    type: gtu::constant const
  - name: type
    type: gtu::constant const
  - name: data
    type: gtu::buffer_ref const&
  return: void
