gl:
  name: glBlendFunci
  params:
  - mapping: buf
    name: buf
    type: GLuint
  - mapping: sfactor
    name: sfactor
    type: GLenum
  - mapping: dfactor
    name: dfactor
    type: GLenum
  return: void
