gl:
  name: glPointParameterfv
  params:
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&value[0]'
    name: params
    type: GLfloat const*
  return: void
gtu:
  name: point_parameter
  params:
  - name: property
    type: gtu::constant const
  - name: value
    type: gtu::vec3< float32 > const&
  return: void
