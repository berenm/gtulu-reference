gl:
  name: glMultiTexCoord1i
  params:
  - mapping: texture_unit
    name: target
    type: GLenum
  - mapping: coordinates[0]
    name: s
    type: GLint
  return: void
gtu:
  name: multi_tex_coord
  params:
  - name: texture_unit
    type: gtu::texture_unit const
  - name: coordinates
    type: gtu::vec1< int32_t > const&
  return: void
  skip: true
