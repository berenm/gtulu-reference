gl:
  name: glPointParameteriv
  params:
  - mapping: property
    name: pname
    type: GLenum
  - mapping: '&values[0]'
    name: params
    type: GLint const*
  return: void
gtu:
  name: point_parameter
  params:
  - name: property
    type: gtu::constant const
  - name: values
    type: gtu::vec3< int32_t > const&
  return: void
