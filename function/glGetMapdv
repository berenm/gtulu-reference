gl:
  name: glGetMapdv
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: query
    name: query
    type: GLenum
  - mapping: '&v[0]'
    name: v
    type: GLdouble*
  return: void
gtu:
  name: get_map
  params:
  - name: target
    type: gtu::constant const
  - name: query
    type: gtu::constant const
  - name: v
    type: std::vector< float64 >&
  return: void
