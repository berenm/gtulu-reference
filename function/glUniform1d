gl:
  name: glUniform1d
  params:
  - mapping: uniform
    name: location
    type: GLint
  - mapping: values[0]
    name: v0
    type: GLdouble
  return: void
gtu:
  name: uniform
  params:
  - name: uniform
    type: gtu::uniform const
  - name: values
    type: gtu::vec1< float64 > const&
  return: void
