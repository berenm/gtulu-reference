gl:
  name: glInterleavedArrays
  params:
  - mapping: format
    name: format
    type: GLenum
  - mapping: stride
    name: stride
    type: GLsizei
  - mapping: pointer
    name: pointer
    type: GLvoid const*
  return: void
gtu:
  name: interleaved_arrays
  params:
  - name: format
    type: gtu::constant const
  - name: stride
    type: gtu::size const
  - name: pointer
    type: gtu::buffer_ref const&
  return: void
