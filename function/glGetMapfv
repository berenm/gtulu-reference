gl:
  name: glGetMapfv
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: query
    name: query
    type: GLenum
  - mapping: v
    name: v
    type: GLfloat*
  return: void
gtu:
  name: get_map
  params:
  - name: target
    type: gtu::constant const
  - name: query
    type: gtu::constant const
  - name: v
    type: float32*
  return: void
