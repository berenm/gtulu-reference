gl:
  name: glVertexAttribI4bv
  params:
  - mapping: vertex_attrib
    name: index
    type: GLuint
  - mapping: '&values[0]'
    name: v
    type: GLbyte const*
  return: void
gtu:
  name: vertex_attrib_integer
  params:
  - name: vertex_attrib
    type: gtu::vertex_attrib const
  - name: values
    type: gtu::vec4< int8_t > const&
  return: void
