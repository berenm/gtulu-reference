gl:
  name: glRects
  params:
  - mapping: x1
    name: x1
    type: GLshort
  - mapping: y1
    name: y1
    type: GLshort
  - mapping: x2
    name: x2
    type: GLshort
  - mapping: y2
    name: y2
    type: GLshort
  return: void
gtu:
  name: rect
  params:
  - name: x1
    type: int16_t const
  - name: y1
    type: int16_t const
  - name: x2
    type: int16_t const
  - name: y2
    type: int16_t const
  return: void
