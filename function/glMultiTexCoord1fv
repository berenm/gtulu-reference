gl:
  name: glMultiTexCoord1fv
  params:
  - mapping: texture_unit
    name: target
    type: GLenum
  - mapping: '&coordinates[0]'
    name: v
    type: GLfloat const*
  return: void
gtu:
  name: multi_tex_coord
  params:
  - name: texture_unit
    type: gtu::texture_unit const
  - name: coordinates
    type: gtu::vec1< float32 > const&
  return: void
