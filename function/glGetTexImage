gl:
  name: glGetTexImage
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: level
    name: level
    type: GLint
  - mapping: format
    name: format
    type: GLenum
  - mapping: type
    name: type
    type: GLenum
  - mapping: data
    name: img
    type: GLvoid*
  return: void
gtu:
  name: get_tex_image
  params:
  - name: target
    type: gtu::constant const
  - name: level
    type: int32_t const
  - name: format
    type: gtu::constant const
  - name: type
    type: gtu::constant const
  - name: data
    type: gtu::bytearray&
  return: void
