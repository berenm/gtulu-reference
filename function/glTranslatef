gl:
  name: glTranslatef
  params:
  - mapping: direction[0]
    name: x
    type: GLfloat
  - mapping: direction[1]
    name: y
    type: GLfloat
  - mapping: direction[2]
    name: z
    type: GLfloat
  return: void
gtu:
  name: translate
  params:
  - name: direction
    type: gtu::vec3< float32 > const&
  return: void
