gl:
  name: glFramebufferRenderbuffer
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: attachment
    name: attachment
    type: GLenum
  - mapping: renderbuffertarget
    name: renderbuffertarget
    type: GLenum
  - mapping: renderbuffer
    name: renderbuffer
    type: GLuint
  return: void
gtu:
  name: framebuffer_renderbuffer
  params:
  - name: target
    type: gtu::constant const
  - name: attachment
    type: gtu::constant const
  - name: renderbuffertarget
    type: gtu::constant const
  - name: renderbuffer
    type: gtu::renderbuffer const
  return: void
