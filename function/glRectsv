gl:
  name: glRectsv
  params:
  - mapping: '&v1[0]'
    name: v1
    type: GLshort const*
  - mapping: '&v2[0]'
    name: v2
    type: GLshort const*
  return: void
gtu:
  name: rect
  params:
  - name: v1
    type: gtu::vec2< int16_t > const&
  - name: v2
    type: gtu::vec2< int16_t > const&
  return: void
