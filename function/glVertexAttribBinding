gl:
  name: glVertexAttribBinding
  params:
  - mapping: vertex_attrib
    name: attribindex
    type: GLuint
  - mapping: vertex_binding
    name: bindingindex
    type: GLuint
  return: void
gtu:
  name: vertex_attrib_binding
  params:
  - name: vertex_attrib
    type: gtu::vertex_attrib const
  - name: vertex_binding
    type: gtu::vertex_binding const
  return: void
