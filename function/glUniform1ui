gl:
  name: glUniform1ui
  params:
  - mapping: uniform
    name: location
    type: GLint
  - mapping: values[0]
    name: v0
    type: GLuint
  return: void
gtu:
  name: uniform
  params:
  - name: uniform
    type: gtu::uniform const
  - name: values
    type: gtu::vec1< uint32_t > const&
  return: void
