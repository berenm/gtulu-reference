gl:
  name: glMapBufferRange
  params:
  - mapping: target
    name: target
    type: GLenum
  - mapping: offset
    name: offset
    type: GLintptr
  - mapping: length
    name: length
    type: GLsizeiptr
  - mapping: access.to_ulong()
    name: access
    type: GLbitfield
  return: void*
gtu:
  name: map_buffer_range
  params:
  - name: target
    type: gtu::constant const
  - name: offset
    type: std::ptrdiff_t const
  - name: length
    type: std::ptrdiff_t const
  - name: access
    type: gtu::bitmask const&
  return: void*
