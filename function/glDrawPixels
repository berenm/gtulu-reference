gl:
  name: glDrawPixels
  params:
  - mapping: dimension[0]
    name: width
    type: GLsizei
  - mapping: dimension[1]
    name: height
    type: GLsizei
  - mapping: format
    name: format
    type: GLenum
  - mapping: type
    name: type
    type: GLenum
  - mapping: data
    name: data
    type: GLvoid const*
  return: void
gtu:
  name: draw_pixels
  params:
  - name: dimension
    type: gtu::vec2< gtu::size > const&
  - name: format
    type: gtu::constant const
  - name: type
    type: gtu::constant const
  - name: data
    type: gtu::buffer_ref const&
  return: void
