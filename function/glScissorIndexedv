gl:
  name: glScissorIndexedv
  params:
  - mapping: index
    name: index
    type: GLuint
  - mapping: '&values[0]'
    name: v
    type: GLint const*
  return: void
gtu:
  name: scissor
  params:
  - name: index
    type: uint32_t const
  - name: values
    type: gtu::vec4< int32_t > const&
  return: void
