gl:
  name: glGetIntegeri_v
  params:
  - mapping: property
    name: pname
    type: GLenum
  - mapping: index
    name: index
    type: GLuint
  - mapping: '&data[0]'
    name: data
    type: GLint*
  return: void
gtu:
  name: get
  params:
  - name: property
    type: gtu::constant const
  - name: index
    type: uint32_t const
  - name: data
    type: std::vector< int32_t >&
  return: void
